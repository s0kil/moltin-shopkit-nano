{
  "name": "moltin-shopkit-femto",
  "version": "0.0.1",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "server:local": "http-server ./dist -a 0.0.0.0 -p 8000 --brotli --gzip --silent",
    "server:localtunnel": "lt --local-host 0.0.0.0 --port 8000",
    "start": "run-p server:local",
    "start:dev": "sirv --dev --host=0.0.0.0 --port=8000 dist",
    "start:test": "browser-sync start --server 'public' --host=0.0.0.0 --port 8000 --https --no-open --no-ui",
    "compress": "run-p compress:gzip compress:brotli",
    "compress:gzip": "gzip --best --keep --force -- dist/app.js",
    "compress:brotli": "brotli --best --keep --force -- dist/app.js"
  },
  "author": "s0kil",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.9.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "http-server": "^0.11.1",
    "loadjs": "^3.6.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-minify-html-literals": "^1.2.2",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.9.0",
    "rollup-plugin-sizes": "^0.5.1",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.5.1"
  },
  "dependencies": {
    "@moltin/request": "^2.0.1",
    "@storeon/localstorage": "^0.5.1",
    "@storeon/svelte": "^0.3.0",
    "fastdom": "^1.0.9",
    "mitt": "^1.1.3",
    "on-idle": "^3.1.4",
    "stage0": "0.0.24",
    "storeon": "^0.8.5",
    "when-dom-ready": "^1.2.12"
  }
}
